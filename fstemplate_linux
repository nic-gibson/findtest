				Dev:       {{.Dev}},       
				Ino:	   {{.Ino}},    
				Nlink:	   {{.Nlink}},
				Mode:	   {{printf "%X" .Mode}},	
				Uid:	   {{.Uid}},
				Gid:	   {{.Gid}},
				Rdev:	   {{.Rdev}},
				Size:      {{.Size}},
				Blksize:   {{.Blksize}},
				Blocks:    {{.Blocks}},
				Atim:      {{template "timespec" .Atim}},
				Mtim:      {{template "timespec" .Mtim}},
				Ctim:      {{template "timespec" .Ctim}},
			},{{end}}
{{define "timespec"}}syscall.Timespec{Sec:  {{.Sec}},Nsec: {{.Nsec}}}{{end}}
